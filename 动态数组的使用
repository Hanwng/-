#include<vector>
using namespace std;

int main()
{
	vector<int> a;//[]定义动态数组
	a.push_back(1);//[1]
	a.push_back(2);//[1,2]
	a.push_back(4);//[1,2,4]
	a[1] = 1;
	a[2] = 1;//修改动态数组的值
	a.pop_back();//删除最后一个元素
	for (int i=0;i<a.size(); i++)
	{
		cout << a[i] << ' ';
	}
	a.clear();
	for (int i = 0; i < a.size(); i++)
	{
		cout << a[i] << ' ';
	}
	return 0;
}
